from __future__ import unicode_literals
2	
3	from django.db import models
4	from django.utils import timezone
5	
6	# Create your models here.
7	class Article(models.Model):
8		title = models.CharField(max_length = 100)
9		category = models.CharField(max_length = 50,blank=True)
10		date_time = models.DateTimeField(default = timezone.now())
11		content = models.TextField(blank =True,null=True)
12		username = models.CharField(max_length = 100,default='SOME STRING')
13	
14		def __str__(self):
15			return self.title
16	
17	class User(models.Model):
18		username = models.CharField(max_length = 100,default='SOME STRING')
19		email = models.EmailField(max_length = 254)
20		password = models.CharField(max_length=100)
21		user_id = models.BigIntegerField(default = -1)
22	
23	
24		def __str__(self):
25			return self.username
